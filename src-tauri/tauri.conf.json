{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "enteract",
  "version": "0.1.0",
  "identifier": "com.enteract.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Enteract Control Panel",
        "width": 320,
        "height": 60,
        "minWidth": 320,
        "minHeight": 60,
        "maxWidth": 900,
        "maxHeight": 1600,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "backgroundColor": "#00000000",
        "center": true,
        "resizable": true,
        "shadow": false,
        "visibleOnAllWorkspaces": true,
        "skipTaskbar": true,
        "fullscreen": false,
        "maximized": false,
        "closable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
